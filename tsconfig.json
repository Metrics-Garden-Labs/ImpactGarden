{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "ES2022",
      "ESNext"
    ],
    "allowJs": true,
    "target": "ES2015",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/chatbot/ActionProvider.js", "src/chatbot/config.js", "src/utils/combinedelegates.js"  ],
  "exclude": [
    "node_modules"
  ]
}
